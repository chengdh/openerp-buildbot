        <record id="view_buildbot_data_tree" model="ir.ui.view">
            <field name="name">report.buildbot.data.tree</field>
            <field name="model">report.buildbot.data</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Buildbot Data">
                    <field name="testing_br_name"/>
                    <field name="testing_br_url"/>
                    <field name="databasename"/>
                    <field name="builddir"/>
                    <field name="tester_server_br"/>
                    <field name="tester_addons_br"/>
                    <field name="stabletimer"/>

                </tree>
            </field>
        </record>

        <record id="action_get_buildbot_data" model="ir.actions.act_window">
            <field name="name">Buildbot data</field>
            <field name="res_model">report.buildbot.data</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_buildbot_reporting" name="Buildbot Reporting" parent="buildbot_menu"/>
        <menuitem action="action_get_buildbot_data" id="menu_buildbot_data" parent="menu_buildbot_reporting"/>
    </data>
